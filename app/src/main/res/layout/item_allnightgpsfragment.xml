<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/relativelayout"
        android:layout_width="304dp"
        android:layout_height="146dp"
        android:background="@drawable/radius2"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="16dp">

        <TextView
            android:id="@+id/Vicinity"
            android:layout_width="165dp"
            android:layout_height="24dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="33dp"
            android:layout_marginLeft="33dp"
            android:layout_marginTop="59dp"
            android:layout_marginEnd="106dp"
            android:layout_marginRight="106dp"
            android:layout_marginBottom="63dp"
            android:text="De garde"
            android:textColor="#8E8B8B"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:layout_editor_absoluteX="114dp"
            tools:layout_editor_absoluteY="78dp" />

        <TextView
            android:id="@+id/placeName"
            android:layout_width="253dp"
            android:layout_height="29dp"

            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="26dp"
            android:layout_marginRight="26dp"
            android:layout_marginBottom="101dp"
            android:text="Pharmacie de Douala "
            android:textColor="@color/textcolor"
            android:textSize="16sp"
            android:textStyle="normal"
            tools:layout_editor_absoluteX="114dp"
            tools:layout_editor_absoluteY="78dp" />

        <Button
            android:id="@+id/btnPharmacie2"
            android:layout_width="122dp"
            android:layout_height="23dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="29dp"
            android:layout_marginLeft="29dp"
            android:layout_marginTop="108dp"
            android:layout_marginEnd="153dp"
            android:layout_marginRight="153dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/radius"
            android:drawableLeft="@drawable/icons8_route_100px"
            android:gravity="center_vertical"
            android:text="Direction"
            android:textAlignment="center"
            android:textColor="@color/blanc"
            android:textSize="12dp" />

        <Button
            android:id="@+id/BtnCallPharmacy"
            android:layout_width="115dp"
            android:layout_height="25dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="171dp"
            android:layout_marginLeft="171dp"
            android:layout_marginTop="106dp"
            android:layout_marginEnd="18dp"
            android:layout_marginRight="18dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/radius2"
            android:drawableLeft="@drawable/icons8_phone_100px_1"
            android:gravity="center_vertical"

            android:text="Appeler"
            android:textAlignment="center"
            android:textColor="@color/textHintColor"
            android:textSize="10dp" />

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>













